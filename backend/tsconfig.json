{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "skipLibCheck": true
  },
  "include": ["resolvers", "types", "src"],
  "exclude": ["node_modules", "dist", "terraform"]
}
